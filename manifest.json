{
  "manifest_version": 2,
  "name": "pan-baidu-helper",
  "version": "0.0.1",
  "description": "Developkit for chrome.",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "pan-baidu-helper"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://pan.baidu.com/*"
      ],
      "js": [
        "lib/jquery/jquery.min.js",
        "scripts/content.js"
      ]
    }
  ],
  "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://cdn.bootcss.com; object-src 'self' ;"
}
